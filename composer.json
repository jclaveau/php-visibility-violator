{
    "name": "jclaveau/php-visibility-violator",
    "description": "Class providing simple helpers to violate the defined visibility of methods and properties",
    "license": "MIT",
    "keywords": ["visibility", "violate", "helpers", "reflection", "private", "protected", "public", "hidden"],
    "authors": [
        {
            "name": "Jean Claveau",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4" : {
            "JClaveau\\VisibilityViolator\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "JClaveau\\VisibilityViolator\\Tests\\" : "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "require": {
        "php": ">=5.6.0"
    }
}
